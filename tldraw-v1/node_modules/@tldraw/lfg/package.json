{
  "name": "@tldraw/lfg",
  "version": "1.1.5",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "bin": {
    "lfg": "./dist/cli.js"
  },
  "files": [
    "dist/**/*"
  ],
  "description": "Build and dev tooling for TypeScript packages.",
  "author": "@steveruizok",
  "scripts": {
    "dev": "ts-node ./src/cli.ts -d -n",
    "build": "ts-node ./src/cli.ts -n",
    "build:packages": "yarn build",
    "test": "jest",
    "prepublish": "yarn build && yarn test",
    "clean": "rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.18",
    "tsconfig-paths": "^3.12.0",
    "tsconfig-replace-paths": "^0.0.11",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.4.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.16",
    "jest": "^27.5.1",
    "prettier-plugin-jsdoc": "^0.3.31",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "jest": {
    "preset": "config/jest/node",
    "moduleNameMapper": {
      "^~(.*)": "<rootDir>/src/$1"
    }
  }
}
